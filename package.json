{
  "name": "vite-electron-temp",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --mode development",
    "build:prod": "npm run typecheck && electron-vite build --mode production",
    "build:beta": "npm run typecheck && electron-vite build --mode beta",
    "postinstall": "electron-builder install-app-deps",
    "build_prod:win": "npm run build:prod && electron-builder --win --config electron-builder.yml",
    "build_prod:mac": "npm run build:prod && electron-builder --mac --config electron-builder.yml",
    "build_prod:linux": "npm run build:prod && electron-builder --linux --config electron-builder.yml",
    "build_beta:win": "npm run build:beta && electron-builder --win --config electron-builder-beta.yml"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/node": "^18.19.22",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "electron": "^28.2.6",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.1.0",
    "element-plus": "^2.6.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "prettier": "^3.2.5",
    "qs": "^6.12.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.71.1",
    "typescript": "^5.4.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.1.5",
    "vue": "^3.4.21",
    "vue-i18n": "^9.10.1",
    "vue-router": "^4.3.0",
    "vue-tsc": "^1.8.27"
  }
}

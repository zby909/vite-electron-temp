{
  "name": "vite-electron-temp",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --mode development",
    "build:prod": "npm run typecheck && electron-vite build --mode production",
    "build:beta": "npm run typecheck && electron-vite build --mode beta",
    "postinstall": "electron-builder install-app-deps",
    "build_prod:win": "npm run build:prod && electron-builder --win --config electron-builder.yml",
    "build_prod:mac": "npm run build:prod && electron-builder --mac --config electron-builder.yml",
    "build_prod:linux": "npm run build:prod && electron-builder --linux --config electron-builder.yml",
    "build_beta:win": "npm run build:beta && electron-builder --win --config electron-builder-beta.yml"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^1.0.27",
    "element-plus": "^2.4.0",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "prettier": "^3.0.2",
    "qs": "^6.11.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.3",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.8"
  }
}

{
  "name": "vite-electron-temp",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --mode development",
    "build:prod": "npm run typecheck && electron-vite build --mode production",
    "build:beta": "npm run typecheck && electron-vite build --mode beta",
    "postinstall": "electron-builder install-app-deps",
    "build_prod:win": "npm run build:prod && electron-builder --win --config electron-builder.yml",
    "build_prod:mac": "npm run build:prod && electron-builder --mac --config electron-builder.yml",
    "build_prod:linux": "npm run build:prod && electron-builder --linux --config electron-builder.yml",
    "build_beta:win": "npm run build:beta && electron-builder --win --config electron-builder-beta.yml"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.9"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.0",
    "axios": "^1.11.0",
    "dayjs": "^1.11.10",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^4.0.0",
    "element-plus": "^2.10.7",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "eslint-plugin-vue": "^10.3.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "prettier": "^3.6.2",
    "qs": "^6.12.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.40.0",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.0.5",
    "vue": "^3.5.17",
    "vue-eslint-parser": "^10.2.0",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.3"
  }
}
